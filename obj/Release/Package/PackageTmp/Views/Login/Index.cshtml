@{
    //Layout = "~/Views/Shared/MainLayout.cshtml";
    ViewBag.Title = "Index";
}
<script src="~/Scripts/jquery-1.8.2.js"></script>
<script src="~/Scripts/hxmd5.js"></script>
<link href="~/Content/logincss.css" rel="stylesheet" />
<div class="container">
    <div style="width: 400px; margin: auto; margin-top: 200px" id="login">
        <form method="post" action="~/Login/LoginCheck">
            <span class="fontawesome-user"></span><input type="text" id="username" name="username" value="@(ViewBag.Username)" onblur="if (this.value === '') this.value = 'Username'" onfocus="if (this.value === 'Username') this.value = ''" required/>
            <br/>
            <span class="fontawesome-lock"></span><input type="password" id="password" name="password" value="@(ViewBag.Password)" onblur="if (this.value === '') this.value = 'Password'" onfocus="if (this.value === 'Password') this.value = ''" required/>
            <br/>
            <input type="submit" id="submit" value="登录" onclick="checknull()"/>
            <br/>
            <div>
                <label id="msg">@ViewBag.Msg</label>
            </div>
        </form>
        <script type="text/javascript">
            $("input#submit").click(function() {
                $("input#password").val(hex_md5($("input#password").val().toString()));
            })
        </script>
    </div>
</div>

<script type="text/javascript">
    function checknull() {
        
    }
</script>
