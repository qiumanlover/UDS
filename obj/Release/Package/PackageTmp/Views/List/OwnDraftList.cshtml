@model IEnumerable<UDS.Models.InfoList>
<link href="~/Content/listulstyle.css" rel="stylesheet" />
<div style="height:500px">
    <ul style="list-style: none; min-width: 600px;">
        <li style="width: 35%; float: left; text-align: center">文档标题</li>
        <li style="width: 20%; float: left; text-align: center">流程名称</li>
        <li style="width: 20%; float: left; text-align: center">填写时间</li>
        <li style="width: 10%; float: left; text-align: center">当前状态</li>
    </ul>
    <br />
    <div id="links">
        @foreach (var item in Model)
        {
            <ul style="min-width:600px;">
                <li style="width:35%;float:left;text-align:center">
                    @Ajax.ActionLink(item.FormTitle, "DraftContainer", "Detail", new { formflowid = item.Id, show = 1, isOld = 1, paras = ViewData["paras"] }, new AjaxOptions() { UpdateTargetId = "replace", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" })
                </li>
                <li style="width:20%;float:left;text-align:center">
                    @Html.DisplayFor(modelItem => item.FormName)
                </li>
                <li style="width:20%;float:left;text-align:center">
                    @Html.DisplayFor(modelItem => item.WriteTime)
                </li>
                <li style="width: 10%; float: left; text-align: center">
                    @Html.DisplayFor(modelItem => item.CurrentState)
                </li>
                <li style="clear: both"></li>
            </ul>
        }
    </div>
</div>
<br />
<div style="width:100%; text-align:center; min-width:400px;">
    @Ajax.ActionLink("首页", "OwnDraftList", "List", new { pageindex = @ViewBag.frtIndex }, new AjaxOptions() { UpdateTargetId = "replace", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" })
    <span>&nbsp;&nbsp;&nbsp;&nbsp;</span>
    @Ajax.ActionLink("上一页", "OwnDraftList", "List", new { pageindex = @ViewBag.preIndex }, new AjaxOptions() { UpdateTargetId = "replace", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" })
    <span>&nbsp;&nbsp;&nbsp;&nbsp;</span>
    第 @ViewBag.pageIndex / @ViewBag.pageCount 页
    <span>&nbsp;&nbsp;&nbsp;&nbsp;</span>
    @Ajax.ActionLink("下一页", "OwnDraftList", "List", new { pageindex = @ViewBag.nxtIndex }, new AjaxOptions() { UpdateTargetId = "replace", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" })
    <span>&nbsp;&nbsp;&nbsp;&nbsp;</span>
    @Ajax.ActionLink("尾页", "OwnDraftList", "List", new { pageindex = @ViewBag.pageCount }, new AjaxOptions() { UpdateTargetId = "replace", InsertionMode = InsertionMode.Replace, HttpMethod = "Post" })
</div>
